<?xml version="1.0" encoding="utf-8"?>
<mx:Panel xmlns:mx="http://www.adobe.com/2006/mxml" width="80%" height="80%" title="Console">
<mx:Script>
	<![CDATA[
		public function write(str:String):void
		{
			textarea.text += str + "\n";
		}
	]]>
</mx:Script>
	<mx:Label text="Hello, World">
		
	</mx:Label>
	<mx:TextArea id="textarea" width="100%" height="100%" />
</mx:Panel>
